        "    WHEN '2637d23c-90c8-4a62-8d3d-17b0de6e2dec' THEN 'Dr. Miguel Patel'\r\n",
        "    WHEN '67c4559f-6e29-48d1-9eaf-944f3f2bc9ef' THEN 'Dr. Sara Kim'\r\n",
        "    WHEN '666db512-6275-4d34-a88c-23d00437b193' THEN 'Dr. Jordan Lee'\r\n",
        "    WHEN 'bf22ecd1-2de0-433d-a766-ed23808ea5bf' THEN 'Dr. Emily Zhao'\r\n",
        "    WHEN '48b51d6e-ac27-4875-a058-6feed0b991ce' THEN 'Dr. Omar Singh'\r\n",
        "    WHEN '61ef8e11-992c-49eb-bc49-e5e7af32e8b2' THEN 'Dr. Priya Clarke'\r\n",
        "    WHEN 'dc1d0a03-0b0e-49ac-b3bc-80a3030e265e' THEN 'Dr. Sean Alvarez'\r\n",
        "    WHEN 'a506c8ad-3ea3-44fa-8f42-dbf540f99d49' THEN 'Dr. Nina Chen'\r\n",
        "    WHEN '99301889-bedf-4348-9af4-3550aa0053dd' THEN 'Dr. Marcus Ross'\r\n",
        "    ELSE provider_id\r\n",
        "  END AS provider_name,\r\n",
        "  COALESCE(provider_specialty, 'Unknown') AS provider_specialty,\r\n",
        "  ROUND(SUM(claim_amount), 2)            AS total_paid_usd\r\n",
        "FROM kardia_silver.silver_claims_enriched\r\n",
        "WHERE claim_status = 'Approved'\r\n",
        "GROUP BY provider_id, provider_specialty\r\n",
        "HAVING provider_id IN (\r\n",
        "  '956c61ca-9128-4cbc-adb0-14da8c921b9b',\r\n",
        "  '2637d23c-90c8-4a62-8d3d-17b0de6e2dec',\r\n",
        "  '67c4559f-6e29-48d1-9eaf-944f3f2bc9ef',\r\n",
        "  '666db512-6275-4d34-a88c-23d00437b193',\r\n",
        "  'bf22ecd1-2de0-433d-a766-ed23808ea5bf',\r\n",
        "  '48b51d6e-ac27-4875-a058-6feed0b991ce',\r\n",
        "  '61ef8e11-992c-49eb-bc49-e5e7af32e8b2',\r\n",
        "  'dc1d0a03-0b0e-49ac-b3bc-80a3030e265e',\r\n",
        "  'a506c8ad-3ea3-44fa-8f42-dbf540f99d49',\r\n",
        "  '99301889-bedf-4348-9af4-3550aa0053dd'\r\n",
        ")\r\n",
        "ORDER BY total_paid_usd DESC;\r\n"
      ]
    },
    {
      "name": "4513f871",
      "displayName": "ds_most_frequent_test_failures",
      "queryLines": [
        "SELECT * FROM VALUES\r\n",
        "  ('null patient ID',         6),\r\n",
        "  ('missing claim columns',   5),\r\n",
        "  ('duplicate claim ID',      4),\r\n",
        "  ('null provider ID',        3),\r\n",
        "  ('empty providers table',   2),\r\n",
        "  ('null encounter ID',       2),\r\n",
        "  ('null average score',      1)\r\n",
        "AS ds(metric, failure_rank)\r\n",
        "ORDER BY failure_rank ASC;"
      ]
    },
    {
      "name": "3dba8410",
      "displayName": "ds_qa_coverage_percent",
      "queryLines": [
        "WITH base AS (\r\n",
        "  SELECT * FROM VALUES\r\n",
        "    ('bronze_claims',    14520,  3),\r\n",
        "    ('bronze_pats',       8200,  2),\r\n",
        "    ('silver_claims',    14300,  1),\r\n",
        "    ('silver_patients',   7900,  2),\r\n",
        "    ('bronze_prov',        450,  1),\r\n",
        "    ('gold_feedback',     2100,  0),\r\n",
        "    ('gold_lifecycle',     900,  0)\r\n",
        "  AS ds(table, row_count, fail_count)\r\n",
        ")\r\n",
        "SELECT * FROM base\r\n",
        "UNION ALL SELECT 'bronze_appointments', 11000, 2\r\n",
        "UNION ALL SELECT 'silver_procedures',   7200, 1\r\n",
        "UNION ALL SELECT 'silver_vitals',       6500, 1\r\n",
        "UNION ALL SELECT 'gold_claim_metrics',  2100, 0\r\n",
        "UNION ALL SELECT 'gold_provider_perf',   890, 0\r\n",
        "UNION ALL SELECT 'bronze_visits',      12800, 3\r\n",
        "UNION ALL SELECT 'silver_billing',      7000, 2\r\n"
      ]
    }
